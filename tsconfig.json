{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "noEmit": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": ["vitest/globals"]
   },
   "exclude": [
      "node_modules", 
      "build",
   ]
}